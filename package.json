{
  "name": "coffeekraken-s-tooltip-component",
  "version": "1.0.0",
  "description": "Full stack tooltip styling supporting multiple sides and colors. Fully customizable as well.",
  "main": "dist/index.js",
  "homepage": "http://coffeekraken.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/coffeekraken/s-tooltip-component"
  },
  "watch": {
    "dist:js": {
      "paths": "src/**/*.js"
    },
    "doc:js": {
      "paths": "src/**/*.js"
    },
    "doc:css": {
      "paths": "src/**/*.scss"
    },
    "demo": {
      "paths": "code-playground.config.js"
    }
  },
  "scripts": {
    "dist": "npm run dist:js",
    "dist:js": "babel src -d dist",
    "demo": "coffeekraken-code-playground",
    "doc": "rm -rf doc && npm run doc:js && npm run doc:css",
    "doc:js": "coffeekraken-docblock-to-markdown -f 'src/**/*.js' -d doc --remove-path src/",
    "doc:css": "coffeekraken-docblock-to-markdown -f 'src/**/*.scss' -d doc --remove-path src/",
    "test": "karma start",
    "start": "coffeekraken-scripts-stack -i dist -i start"
  },
  "keywords": [
    "tooltip",
    "customizable",
    "webcomponent",
    "component",
    "html",
    "sugar",
    "coffee",
    "kraken",
    "coffeekraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "contributors": [
    {
      "name": "Olivier Bossel",
      "email": "olivier.bossel@gmail.com",
      "url": "http://olivierbossel.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "coffeekraken-sugar": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-default": "^7.0.0-alpha.20",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "coffeekraken-code-playground": "^1.0.0",
    "coffeekraken-docblock-to-markdown": "^1.0.0",
    "coffeekraken-s-button-component": "^1.0.0",
    "coffeekraken-s-typography-component": "^1.0.0",
    "coffeekraken-scripts-stack": "^1.0.1",
    "http-server": "^0.9.0",
    "karma": "^2.0.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.0",
    "node-sass": "^4.0.0",
    "webcomponents.js": "^0.7.24",
    "webpack": "^4.0.0"
  }
}
